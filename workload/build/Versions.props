<Project>
  <!--
    Workload version:
    * The first two numbers represent the major and minor version of the Tizen OS.
    * The third number is the sdk version band with release count.
    * However, the first digit of the third number starts at 100.

    Pre-release version:
    1) CI Versions
       format: -ci.[branch name | gh + #PR].[Commit Distance]+sha[Commit Hash]
       example: -ci.main.100+sha12345
                -ci.gh23.100+sha12345
    2) Official Versions:
       format: -[preview name].[Commit Distance]+sha[Commit Hash]
       example: -preview.3.100+sha12345
  -->
  <PropertyGroup>
    <TizenWorkloadVersion>7.0.100</TizenWorkloadVersion>
  </PropertyGroup>

  <!-- 
    TizenFX API version:
    * The first number represents the API Level.
    * The sencond and third numbers represents the reserved version.
    * The fourth number represents the build number.
    This version is used to generate Samsung.Tizen.Ref package.
  -->
  <PropertyGroup>
    <TizenFXVersion>10.0.0.17305</TizenFXVersion>
  </PropertyGroup>

  <!--
    Dotnet SDK versions for building the workload.
   -->
  <PropertyGroup>
    <MicrosoftDotnetSdkInternalPackageVersion>7.0.100-rc.1.22362.32</MicrosoftDotnetSdkInternalPackageVersion>
    <MicrosoftDotNetBuildTasksFeedPackageVersion>7.0.0-beta.22103.1</MicrosoftDotNetBuildTasksFeedPackageVersion>
  </PropertyGroup>
</Project>
