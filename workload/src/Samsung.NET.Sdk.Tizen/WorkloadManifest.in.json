{
  "version": "@TIZEN_WORKLOAD_VERSION@",
  "workloads": {
    "tizen": {
      "description": ".NET SDK Workload for building Tizen applications.",
      "packs": [
        "Samsung.Tizen.Sdk",
        "Samsung.Tizen.Sdk.net6",
        "Samsung.Tizen.Sdk.net7",
        "Samsung.Tizen.Ref",
        "Samsung.Tizen.Templates",
        "Samsung.NETCore.App.Runtime.tizen"
      ]
    }
  },
  "packs": {
    "Samsung.Tizen.Sdk": {
      "kind": "sdk",
      "version": "@TIZEN_WORKLOAD_VERSION@"
    },
    "Samsung.Tizen.Sdk.net6": {
      "kind": "sdk",
      "version": "9.0.101",
      "alias-to": {
        "any": "Samsung.Tizen.Sdk"
      }
    },
    "Samsung.Tizen.Sdk.net7": {
      "kind": "sdk",
      "version": "@TIZEN_WORKLOAD_VERSION@",
      "alias-to": {
        "any": "Samsung.Tizen.Sdk"
      }
    },
    "Samsung.Tizen.Ref": {
      "kind": "framework",
      "version": "@TIZEN_WORKLOAD_VERSION@"
    },
    "Samsung.Tizen.Templates": {
      "kind": "template",
      "version": "@TIZEN_WORKLOAD_VERSION@"
    },
    "Samsung.NETCore.App.Runtime.tizen": {
      "kind": "framework",
      "version": "@TIZEN_WORKLOAD_VERSION@"
    }
  }
}
