<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <_TizenFrameworkReferenceVersion>@TIZEN_PACK_VERSION@</_TizenFrameworkReferenceVersion>
  </PropertyGroup>

  <ItemGroup>
    <KnownFrameworkReference
      Include="Samsung.Tizen"
      TargetFramework="net6.0"
      RuntimeFrameworkName="Samsung.Tizen"
      DefaultRuntimeFrameworkVersion="$(_TizenFrameworkReferenceVersion)"
      LatestRuntimeFrameworkVersion="$(_TizenFrameworkReferenceVersion)"
      TargetingPackName="Samsung.Tizen.Ref"
      TargetingPackVersion="$(_TizenFrameworkReferenceVersion)"
      RuntimePackNamePatterns="Samsung.Tizen.Runtime.**RID**"
      RuntimePackRuntimeIdentifiers="tizen-arm;tizen-x86"
      Profile="Tizen"
      IsTrimmable="true"
    />
  </ItemGroup>

</Project>
